{
  "expo": {
    "name": "PiAutoBox",
    "description": "This project is really great.",
    "slug": "PiAutoBox",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "android": {
      "package": "com.river.piAutobox"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.river.piAutobox",
      "supportsTablet": true
    }
  }
}
